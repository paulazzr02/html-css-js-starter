{
  "name": "html-css-starter",
  "version": "1.1.0",
  "description": "효율적인 프론트엔드 개발을 위한 범용 퍼블리싱 템플릿 시스템",
  "main": "index.js",
  "keywords": [
    "publishing",
    "template",
    "scss",
    "bootstrap",
    "gulp",
    "frontend",
    "uiux",
    "starter"
  ],
  "author": "paulazzr02",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulazzr02/html-css-starter.git"
  },
  "bugs": {
    "url": "https://github.com/paulazzr02/html-css-starter/issues"
  },
  "homepage": "https://github.com/paulazzr02/html-css-starter#readme",
  "engines": {
    "node": ">=18.17.1",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development gulp",
    "build": "cross-env NODE_ENV=production gulp prod",
    "clean": "gulp clean",
    "new": "node config/scripts/generate-page.js",
    "serve": "gulp",
    "start": "npm run dev",
    "compile-scss": "npx sass src/styles/styles.scss dist/css/styles.css --no-source-map",
    "tokens:convert": "node config/scripts/convert-custom-tokens.js",
    "format": "prettier --write \"**/*.{js,json,html,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,json,html,css,scss}\"",
    "test:a11y": "node config/scripts/test-accessibility.js"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "cross-env": "^10.1.0",
    "glob": "^11.0.3",
    "gulp": "4",
    "gulp-clean": "^0.4.0",
    "gulp-file-include": "^2.2.2",
    "gulp-rename": "^2.1.0",
    "gulp-sass": "^6.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "pa11y": "^9.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.93.2"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "krds-uiux": "^1.0.5",
    "material-icons": "^1.13.14",
    "pretendard": "^1.3.9"
  }
}
